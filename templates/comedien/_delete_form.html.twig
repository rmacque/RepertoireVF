{% if is_granted('ROLE_ADMIN') %}
<form method="post" action="{{ path('comedien_delete', {'id': comedien.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ comedien.id) }}">
    <button class="btn">Supprimer</button>
</form>
{% endif %}